<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="diseaseService" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="uk.ac.ox.zoo.seeg.abraid.mp.common.service.DiseaseService" />
    </bean>
    <bean id="expertService" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="uk.ac.ox.zoo.seeg.abraid.mp.common.service.ExpertService" />
    </bean>
    <bean id="locationService" class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="uk.ac.ox.zoo.seeg.abraid.mp.common.service.LocationService" />
    </bean>
</beans>
